# Calleroo Scheduler Service Configuration

# Server port
PORT=8090

# Database path (SQLite file)
DATABASE_PATH=./scheduler.db

# Default backend URL to call for executing tasks
DEFAULT_BACKEND_BASE_URL=https://api.callerooapp.com

# Optional: Internal auth token for backend calls
# If not set, calls are made without authentication
BACKEND_INTERNAL_TOKEN=

# Scheduler poll interval in seconds
POLL_INTERVAL=3.0
